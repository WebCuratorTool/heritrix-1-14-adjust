  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <testSourceDirectory>src/java</testSourceDirectory>
    <resources>
      <resource>
        <directory>${basedir}/src/resources</directory>
        <includes>
          <include>*.xsl</include>
        </includes>
      </resource>
      <resource>
        <directory>${project.build.directory}/conf/</directory>
        <includes>
          <include>heritrix.properties</include>
          <include>jndi.properties</include>
          <include>profiles/*default/**</include>
          <include>modules/**</include>
          <include>selftest/**</include>
          <include>effective_tld_names.dat</include>
        </includes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <!-- Corresponds to December 2009 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.1</version>
        <configuration>
          <encoding>UTF-8</encoding>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to August 2011 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <archive>
            <manifest>
              <addExtensions>false</addExtensions>
              <mainClass>org.archive.crawler.Heritrix</mainClass>
            </manifest>
            <manifestFile>${basedir}/src/java/META-INF/MANIFEST-MF</manifestFile>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to May 2010 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-site-plugin</artifactId>
        <version>2.1.1</version>
        <configuration>
          <moduleExcludes>
            <xdoc>navigation.xml</xdoc>
          </moduleExcludes>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to May 2008 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <argLine>-ea</argLine>
          <forkMode>once</forkMode>
          <systemProperties>
            <testtmpdir>${project.build.directory}/test-tmp</testtmpdir>
          </systemProperties>
          <excludes>
            <exclude>**/*SelfTest.java</exclude>
          </excludes>
          <includes>
            <include>**/*Test.java</include>
          </includes>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to October 2010 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.1.1</version>
        <configuration>
          <warSourceDirectory>src/webapps/admin/</warSourceDirectory>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <plugins>
      <plugin>
        <!-- Corresponds to January 2011 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-changes-plugin</artifactId>
        <version>2.4</version>
        <reportSets>
          <reportSet>
            <reports>
              <report>changes-report</report>
            </reports>
          </reportSet>
        </reportSets>
        <configuration>
          <xmlPath>${basedir}/xdocs/changes.xml</xmlPath>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to October 2009 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.6.1</version>
        <configuration>
          <author>true</author>
          <destDir>${project.build.directory}/apidocs</destDir>
          <doctitle>${project.name} ${project.version} </doctitle>
          <links>
            <link>http://java.sun.com/j2se/1.5.0/docs/api</link>
          </links>
          <show>package</show>
          <source>1.5</source>
          <use>true</use>
          <version>true</version>
          <windowtitle>${project.name} ${project.version} </windowtitle>
          <docencoding>UTF-8</docencoding>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <!-- Corresponds to June 2010 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jxr-plugin</artifactId>
        <version>2.2</version>
      </plugin>
      <plugin>
        <!-- Corresponds to May 2008 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>2.4</version>
      </plugin>
      <plugin>
        <!-- Corresponds to May 2008 -->
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-report-plugin</artifactId>
        <version>2.4.3</version>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>taglist-maven-plugin</artifactId>
        <version>2.4</version>
      </plugin>
    </plugins>
  </reporting>
</project>
